{
    "name": "@my-lib/components",
    "version": "0.0.0",
    "type": "module",
    "module": "./dist/main.js",
    "main": "./dist/main.cjs",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/main.js",
            "require": "./dist/main.cjs",
            "types": "./dist/main.d.ts"
        },
        "./client": {
            "types": "./dist/client.d.ts"
        },
        "./index.css": "./dist/style.css"
    },
    "files": [
        "/dist",
        "/client.d.ts"
    ],
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/tsconfig": "^0.1.3",
        "rollup": "^3.20.6",
        "rollup-plugin-dts": "^5.3.0",
        "typescript": "^4.9.3",
        "vite": "^4.2.1",
        "vue": "^3.2.47",
        "vue-tsc": "^1.2.0"
    },
    "peerDependencies": {
        "vue": "^3.2.45"
    },
    "scripts": {
        "lint": "run-s lint:*",
        "lint:type-check-vue": "vue-tsc --noEmit",
        "lint:type-check-ts": "tsc --project ./tsconfig.type-check-ts.json --noEmit",
        "prebuild": "run-s lint:type-check-vue lint:type-check-ts",
        "build": "run-s build:src build:types:*",
        "build:src": "vite build",
        "build:types:build": "vue-tsc -p tsconfig.types.json",
        "build:types:bundle": "rollup -c ./build/rollup.config.js",
        "build:types:clean": "rimraf ./dist/types"
    }
}
