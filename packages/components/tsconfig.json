{
    "extends": "@vue/tsconfig/tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "Node16",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "types": [
            "vite/client"
        ],
        "noEmit": true
    },
    "include": [
        "./src/**/*.ts",
        "./src/**/*.d.ts",
        "./src/**/*.tsx",
        "./src/**/*.vue"
    ]
}
