{
  "name": "moist",
  "version": "1.0.0",
  "description": "Illustrates an issue documented at https://github.com/streetsidesoftware/cspell/issues/1114",
  "main": "index.js",
  "repository": "git@github.com:roydukkey/moist.git",
  "author": "roydukkey <contact@changelog.me>",
  "license": "MIT",
  "scripts": {
    "test:before": "npx cspell@5.3.1 --config './.vscode/cSpell.json' '**/{.*/**/,.*/**/.,,.}*'",
    "test:after": "npx cspell@5.3.2 --config './.vscode/cSpell.json' '**/{.*/**/,.*/**/.,,.}*'",
    "test:workaround": "npx cspell@5.3.2 --config './.vscode/cSpell-workaround.json' '**/{.*/**/,.*/**/.,,.}*'"
  },
  "dependencies": {
    "clean-package": "^1.0.1"
  }
}
